package codeShef;

import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;

public class braceproblem {
	public static void main(String[] args) {
		List<String> myInputList = new ArrayList<>();
		Scanner scan = new Scanner(System.in);
		int testCase = scan.nextInt();
		String[] inputString = new String[testCase];
		for (int i = 0; i < testCase; i++) {
			inputString[i] = scan.next();
		}
		String output[]=braces(inputString);

	}
	
	 static String[] braces(String[] values) {
		 int openBrace=0,closeBrace=0;
		 for (String str : values) {
			for(int j=0;j<str.length();j++){
				if(str.charAt(j)=='[' || str.charAt(j)=='{' || str.charAt(j)=='(' ){
					openBrace++;
				}
			}
		}
		 
		 
		 	
	        foreach(String inputValues: values){
	            char[] inputChar=inputValues.toCharArray();
	            for(int i=0;i<inputChar.length;i++){
	                for(int j=0;j<openBrace.length;j++){
	                    if(inputChar[i]=openBrace[j]){
	                        openBraceCount++;
	                    }
	                     if(inputChar[i]=closeBrace[j]){
	                        closeBraceCount++;
	                    }   
	                }
	            }
	            int index=0;
	            if(openBraceCount==closeBraceCount){
	                output[index]="Yes";
	                index++;
	            }else{
	                output[index]="No";
	                index++;
	            }
	        }
	        return output;
	    }
}
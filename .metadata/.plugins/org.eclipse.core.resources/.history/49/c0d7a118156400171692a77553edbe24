package codeShef;
import java.util.Arrays;
import java.util.HashSet;
import java.util.Scanner;
import java.util.Set;
public class NumberNeeded {
	
    public static int numberNeeded(String first, String second) {
		Set<Character> mySet = new HashSet<>();
		char[] char2 = second.toCharArray();
		char[] removeChar= new char[first.length()+second.length()];
		
		Arrays.sort(char2);
		String sec = String.valueOf(char2);
		
		for (int f = 0; f < first.length(); f++) {
			mySet.add(first.charAt(f));
			for (int s = 0; s < sec.length(); s++) {
				mySet.add(sec.charAt(s));
				if (first.charAt(f) == sec.charAt(s)) {
					removeChar[f]=first.charAt(f);
					break;
				}
			}
		}
      
		for (int k=0;k<removeChar.length;removeChar++){
			mySet.remove(removeChar[k]);
		}
      return mySet.size();
    }
  
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        String a = in.next();
        String b = in.next();
        System.out.println(numberNeeded(a, b));
    }
}
